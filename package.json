{
  "name": "enso",
  "version": "1.0.0",
  "description": "Atoms-like form state managment library",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "require": {
          "types": "./index.d.cts",
          "default": "./index.cjs"
        },
        "import": {
          "types": "./index.d.ts",
          "default": "./index.js"
        }
      },
      "./form": {
        "require": {
          "types": "./form/index.d.cts",
          "default": "./form/index.cjs"
        },
        "import": {
          "types": "./form/index.d.ts",
          "default": "./form/index.js"
        }
      },
      "./field": {
        "require": {
          "types": "./field/index.d.cts",
          "default": "./field/index.cjs"
        },
        "import": {
          "types": "./field/index.d.ts",
          "default": "./field/index.js"
        }
      }
    }
  },
  "scripts": {
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kossnocorp/enso.git"
  },
  "keywords": [
    "form",
    "state",
    "React"
  ],
  "author": "Sasha Koss <koss@nocorp.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kossnocorp/enso/issues"
  },
  "homepage": "https://github.com/kossnocorp/enso#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.2",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@parcel/watcher": "^2.4.1",
    "@swc/core": "^1.4.13",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitest/browser": "^3.0.3",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "benny": "^3.7.1",
    "bytes-iec": "^3.1.1",
    "glob": "^10.3.12",
    "minimatch": "^10.0.1",
    "picocolors": "^1.0.0",
    "playwright": "^1.49.1",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.3",
    "vitest-browser-react": "^0.0.4"
  },
  "packageManager": "pnpm@9.10.0+sha512.73a29afa36a0d092ece5271de5177ecbf8318d454ecd701343131b8ebc0c1a91c487da46ab77c8e596d6acf1461e3594ced4becedf8921b074fbd8653ed7051c",
  "dependencies": {
    "nanoid": "^5.0.9"
  }
}
