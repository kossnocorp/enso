{
  "name": "enso",
  "version": "1.0.0",
  "description": "Atoms-like form state managment library",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "require": {
          "types": "./index.d.cts",
          "default": "./index.cjs"
        },
        "import": {
          "types": "./index.d.ts",
          "default": "./index.js"
        }
      },
      "./form": {
        "require": {
          "types": "./form/index.d.cts",
          "default": "./form/index.cjs"
        },
        "import": {
          "types": "./form/index.d.ts",
          "default": "./form/index.js"
        }
      },
      "./field": {
        "require": {
          "types": "./field/index.d.cts",
          "default": "./field/index.cjs"
        },
        "import": {
          "types": "./field/index.d.ts",
          "default": "./field/index.js"
        }
      },
      "./change": {
        "require": {
          "types": "./change/index.d.cts",
          "default": "./change/index.cjs"
        },
        "import": {
          "types": "./change/index.d.ts",
          "default": "./change/index.js"
        }
      }
    }
  },
  "scripts": {
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kossnocorp/enso.git"
  },
  "keywords": [
    "form",
    "state",
    "React"
  ],
  "author": "Sasha Koss <koss@nocorp.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kossnocorp/enso/issues"
  },
  "homepage": "https://github.com/kossnocorp/enso#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.2",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint/js": "^9.28.0",
    "@parcel/watcher": "^2.4.1",
    "@swc/core": "^1.4.13",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitest/browser": "^3.2.2",
    "@vitest/ui": "^3.2.2",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "benny": "^3.7.1",
    "bytes-iec": "^3.1.1",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^10.3.12",
    "globals": "^16.2.0",
    "minimatch": "^10.0.1",
    "picocolors": "^1.0.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.2.2",
    "vitest-browser-react": "^0.2.0"
  },
  "packageManager": "pnpm@10.12.2+sha512.a32540185b964ee30bb4e979e405adc6af59226b438ee4cc19f9e8773667a66d302f5bfee60a39d3cac69e35e4b96e708a71dd002b7e9359c4112a1722ac323f",
  "dependencies": {
    "nanoid": "^5.0.9"
  }
}
