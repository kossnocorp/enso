{
  "name": "enso",
  "version": "2.1.0",
  "description": "Maximalist state & form management library for React",
  "type": "module",
  "main": "index.cjs",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "require": {
          "types": "./index.d.cts",
          "default": "./index.cjs"
        },
        "import": {
          "types": "./index.d.ts",
          "default": "./index.js"
        }
      },
      "./dev": {
        "require": {
          "types": "./dev.d.cts",
          "default": "./dev.cjs"
        },
        "import": {
          "types": "./dev.d.ts",
          "default": "./dev.js"
        }
      }
    }
  },
  "scripts": {
    "test": "vitest run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kossnocorp/enso.git"
  },
  "keywords": [
    "form",
    "state",
    "React"
  ],
  "author": "Sasha Koss <koss@nocorp.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kossnocorp/enso/issues"
  },
  "homepage": "https://github.com/kossnocorp/enso#readme",
  "dependencies": {
    "alwaysly": "^0.3.1",
    "nanoid": "^5.1.5",
    "react": "^19"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.2",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/plugin-transform-modules-commonjs": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint/js": "^9.28.0",
    "@parcel/watcher": "^2.4.1",
    "@swc/core": "^1.4.13",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitest/ui": "^3.2.2",
    "babel-plugin-replace-import-extension": "^1.1.4",
    "benny": "^3.7.1",
    "bytes-iec": "^3.1.1",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^10.3.12",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "minimatch": "^10.0.1",
    "picocolors": "^1.0.0",
    "prettier": "^3.5.3",
    "react-dom": "^19.1.1",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.33.1",
    "tyst": "1.0.0-alpha.2",
    "vitest": "^3.2.2"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
